        "identifier" : [ "$$CISBN$$V051100950X;$$CISBN$$V9780511009501;$$CISBN$$V0511033400;$$CISBN$$V9780511033407;$$CISBN$$V0511117957;$$CISBN$$V9780511117954;$$CISBN$$V9780521668071;$$CISBN$$V0521668077;$$CISBN$$V9780521661089;$$CISBN$$V9780511605772;$$CISBN$$V0511605773;$$CISBN$$V0511150857;$$CISBN$$V9780511150852;$$COCLC$$V(OCoLC)52559590;$$CISBN$$V0521661080" ],
