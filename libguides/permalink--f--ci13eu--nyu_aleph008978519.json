        "subject" : [ "Vampires in popular culture -- History", "Vampires -- History", "Vampires -- Histoire", "Vampires dans la culture populaire -- Histoire", "Vampires", "Vampires in popular culture" ],
        "format" : [ "1 online resource" ],
        "identifier" : [ "$$CISBN$$V9781526776631;$$CISBN$$V1526776634;$$CISBN$$V9781526776655;$$CISBN$$V1526776650;$$COCLC$$V(OCoLC)1251765707;$$CISBN$$V1526776626;$$CISBN$$V9781526776624" ],
