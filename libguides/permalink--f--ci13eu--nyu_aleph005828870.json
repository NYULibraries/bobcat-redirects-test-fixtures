        "identifier" : [ "$$CISBN$$V9781429498081;$$CISBN$$V1429498080;$$CISBN$$V9786155211355;$$CISBN$$V6155211353;$$CISBN$$V1281128937;$$CISBN$$V9781281128935;$$CISBN$$V2821815115;$$CISBN$$V9782821815117;$$CISBN$$V9786611128937;$$CISBN$$V661112893X;$$COCLC$$V(OCoLC)648609641" ],
        "creationdate" : [ "2007" ],
