        "title" : [ "Ukraine in crisis " ],
        "subject" : [ "Ukraine Conflict, 2014-", "Ukraine -- Foreign relations -- Europe", "Europe -- Foreign relations -- Ukraine", "National security -- Europe" ],
        "format" : [ "ix, 123 pages : illustrations ; 26 cm" ],
